    <style>
        @font-face{
            font-family: 'diotima';
            src: url('diotima.otf');
        }
        *{
            margin:0;
            padding:0;
            box-sizing:border-box;
        }
        html{
            font-size:10px;
        }
        body{
            font-family: diotima;
            font-size:1rem;
            line-height: 2rem;
        }
        .page{
            padding:0.5in 0;
            margin:5px auto;
            width:8.5in;
            height:11in;
            background:beige;
            border:1px solid black;
        }
        .title-page{
            display:grid;
            padding:0;
            place-content:center;
            font-size:2rem;
        }
        h1,h2,h3,h4,h5{
            font-size:1.2rem;
            font-weight:500;
        }
        h2{
            margin-top:1rem;
        }
        .item{
            display:flex;
            justify-content: space-between;
            width:6in;
            margin:0 auto;
        }
        .item-left{
            display:flex;
        }
        .item-right{
            display:flex;
            margin-left:10px;
        }
        .bin{
            width:0.5in;
            
        }
        .vintage-size,.price{
            width:0.5in;
        }
        .hashtag{
            color:#cccccc00;
        }
    </style>
</head>
